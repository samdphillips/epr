#lang rhombus/static/and_meta

import:
  lib("racket/base.rkt") as r
  "evt.rhm" open
  "supervise.rhm" open

export:
  Processor

class Processor():
  extends Supervisable
  nonfinal

  abstract method process_evt() :~ Evt

  override method process():
    r.sync(stopping_evt.handle_evt(r.void),
           process_evt())
